<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.example.android.layout"
      android:versionCode="1"
      android:versionName="1.0.0">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:label="@string/app_name" android:name="LayoutExample">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="LinearLayoutActivity" android:label="@string/app_name_linear"></activity>
        <activity android:name="RelativeLayoutActivity" android:label="@string/app_name_relative"></activity>
        <activity android:name="TableLayoutActivity" android:label="@string/app_name_table"></activity>
        <activity android:name="AbsoluteLayoutActivity" android:label="@string/app_name_absolute"></activity>
    </application>
</manifest>
 